{
  "name": "project-fast",
  "version": "0.9.1",
  "description": "Project \"Full Application Stack Toolkit\" is a modern frontend framework inspired by Google's Material Design.",
  "homepage": "https://github.com/OverflowHayden/project-fast.git#readme",
  "scripts": {
    "build": "webpack --progress && stylemark -i src -o dist -c stylemark.yml",
    "dev": "concurrently --kill-others \"stylemark -i src -o dist -c stylemark.yml -w 20000 -b\" \"webpack --watch --hot\"",
    "toolkit": "stylemark -i src -o dist -c stylemark.yml -b",
    "test": "npm run toolkit && npm run build"
  },
  "author": "Hayden Lin",
  "license": "LGPL-3.0",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.0.0-beta.51",
    "stylelint": "^9.1.3",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "stylelint-webpack-plugin": "^0.10.3",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/runtime": "^7.0.0-beta.51",
    "@mdi/font": "^2.4.85",
    "autoprefixer": "^8.4.1",
    "babel-loader": "^8.0.0-beta",
    "bourbon": "^5.0.0",
    "compression-webpack-plugin": "^1.1.12",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "pixrem": "^4.0.1",
    "postcss-csso": "^3.0.0",
    "postcss-loader": "^2.1.5",
    "sag-bourbon-neat": "^2.1.1",
    "sass-loader": "^7.0.3",
    "snackbarlightjs": "^1.1.2",
    "stylemark": "0.6.8",
    "tingle.js": "^0.13.2",
    "tippy.js": "^2.5.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": ">3.12.0",
    "webpack-concat-plugin": "^3.0.0"
  }
}
