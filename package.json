{
  "name": "project-fast",
  "version": "2.0.0-alpha",
  "description": "Project \"Full Application Stack Toolkit\" is a modern frontend framework inspired by Google's Material Design.",
  "homepage": "https://github.com/OverflowHayden/project-fast.git#readme",
  "scripts": {
    "build": "webpack --progress && stylemark -i src -o dist -c stylemark.yml",
    "dev": "concurrently --kill-others \"stylemark -i src -o dist -c stylemark.yml -w 5000 -b\" \"webpack --watch --hot\"",
    "toolkit": "stylemark -i src -o dist -c stylemark.yml -b",
    "test": "npm run toolkit && npm run build"
  },
  "main": "dist/src/assets/js/app.js",
  "module": "src/assets/js/app.js",
  "author": "Hayden Lin",
  "license": "LGPL-3.0",
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^6.0.5",
    "concurrently": "^4.1.2",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "image-webpack-loader": "^7.0.1",
    "mini-css-extract-plugin": "^1.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-csso": "^5.0.0",
    "postcss-loader": "^4.0.4",
    "sass-loader": "^10.0.5",
    "stylelint": "^13.7.2",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "stylemark": "^3.1.7",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-concat-files-plugin": "^0.4.1"
  },
  "dependencies": {
    "@iconify/iconify": "^1.0.7",
    "bootstrap-reboot": "^4.4.1",
    "bourbon": "^7.0.0",
    "bourbon-neat": "^4.0.0",
    "choices.js": "^9.0.1",
    "snackbarlightjs": "^1.1.2",
    "terser-webpack-plugin": "^5.0.3",
    "tingle.js": "^0.15.3",
    "tippy.js": "^6.2.7"
  }
}
