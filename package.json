{
  "name": "project-fast",
  "version": "1.0.3",
  "description": "Project \"Full Application Stack Toolkit\" is a modern frontend framework inspired by Google's Material Design.",
  "homepage": "https://github.com/OverflowHayden/project-fast.git#readme",
  "scripts": {
    "build": "webpack --progress && stylemark -i src -o dist -c stylemark.yml",
    "dev": "concurrently --kill-others \"stylemark -i src -o dist -c stylemark.yml -w 5000 -b\" \"webpack --watch --hot\"",
    "toolkit": "stylemark -i src -o dist -c stylemark.yml -b",
    "test": "npm run toolkit && npm run build"
  },
  "main": "dist/src/assets/js/app.js",
  "module": "src/assets/js/app.js",
  "author": "Hayden Lin",
  "license": "LGPL-3.0",
  "browserslist": [
    "last 2 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.0.1",
    "concurrently": "^4.1.2",
    "css-loader": "^3.4.0",
    "file-loader": "^5.0.2",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-csso": "^3.0.0",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^8.0.0",
    "stylelint": "^12.0.1",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "stylelint-webpack-plugin": "^1.1.2",
    "stylemark": "^3.1.3",
    "webpack": ">3.12.0",
    "webpack-cli": "^3.3.10",
    "webpack-concat-plugin": "^3.0.0"
  },
  "dependencies": {
    "@iconify/iconify": "^1.0.3",
    "bootstrap-reboot": "^4.3.1",
    "bourbon": "^6.0.0",
    "bourbon-neat": "^4.0.0",
    "choices.js": "^7.1.5",
    "snackbarlightjs": "^1.1.2",
    "terser-webpack-plugin": "^2.3.5",
    "tingle.js": "^0.15.2",
    "tippy.js": "^4.3.5"
  }
}
